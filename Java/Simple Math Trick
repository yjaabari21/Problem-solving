import java.util.*;
        
public class test {
public static void main(String args[])
{
    Scanner scn = new Scanner(System.in);
    int a = scn.nextInt();
    int b = scn.nextInt();
    int c = a - b;
    if(c>=0)
    {
        System.out.println(c);
    }
   else{
        System.out.println("0");
    }
}
}
