public class test{
    public static void main(String[] args)
    {
    Scanner scn = new Scanner(System.in);
    int num = scn.nextInt();
    int count =0;
    for(int i = 2; i<num; i++)
    {
        if(num%i == 0)
        {
            count++;
        }
    }
    if(count>0)
    {
        System.out.println("NO");
    }
    else
    {
        System.out.println("YES");
    }
   }   
 }
