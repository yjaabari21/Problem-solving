import java.util.*;
public class test{
    public static void main(String args[])
    { 
    Scanner inp = new Scanner(System.in);
    char c = inp.next().charAt(0);
    int s = inp.nextInt();
    int[] arr = new int[s];
    for(int i = 0; i<s; i++)
    {
        arr[i] = inp.nextInt();
        for(int j=0; j<arr[i]; j++)
        {
            System.out.print(c);
        }
        System.out.println();
    }
  }
}
