import java.util.*;
        
public class test {
public static void main(String args[])
{
    Scanner scn = new Scanner(System.in);
    int n = scn.nextInt();
    long sum = 0;
    int[] arr1 = new int[n];
    for(int i = 0; i <n; i++){
        arr1[i] = scn.nextInt();
        sum = arr1[i] + sum ;
   }
    if(sum<0)
    System.out.println(sum * -1);
    else
        System.out.println(sum);
  }
}
