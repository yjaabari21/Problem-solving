import java.util.*;
public class test{
    public static void main(String args[])
    { 
        Scanner scn = new Scanner(System.in);
        int t = scn.nextInt();
        long[] arr = new long[t];
        for(int i = 0; i<arr.length; i++)
        {
            arr[i] = scn.nextLong();
        }
        for(int i =arr.length-1; i>=0; i--)
        {
            System.out.print(arr[i] + " ");
        }
    }
}
