import java.util.*;
        
public class test {
public static void main(String args[])
{
    Scanner scn = new Scanner(System.in);
    for(int i = 0; i <=10000; i++){
    int n = scn.nextInt();
    if(n == 1999)
    {
        System.out.println("Correct");
        break;
    }
    else{
        System.out.println("Wrong");
    }
   }
  }
}
