import java.util.*;
        
public class test {
public static void main(String args[])
{
    Scanner scn = new Scanner(System.in);
    char a = scn.next().charAt(0);
    if(a>=97 && a != 'z')
    {
        int z = (int) a + 1; 
        System.out.println((char)z);
    }
    else if(a == 'z'){
        int z = (int) a - 25;
        System.out.println((char)z);
    }
}
}
